.container-list-btn {
  width: 260rpx;
   height: 240rpx; 

  border-right: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  font-size: 34rpx;
  color: #333;
}
.container-list-btn view {
  width: 200rpx;
  height: 68rpx;
  border-radius: 20rpx;
  margin-bottom: 6rpx;
  /* margin-top: 6rpx; */
  line-height: 68rpx;
  text-align: center;
}
.on {
  background: #33adff;
  color: #fff;
}

.container-list-parameter{
  width: 290rpx;
}
.car-item-title {
  height: 88rpx;
    /* width: 219rpx;         */
  line-height: 88rpx;
  display: block;
  background-color: #fafafa;
  font-size: 34rpx;
  border-right: 1rpx solid #fafafa;
     padding-left:10rpx; 
  color: #33adff!important;
}

.car-item-list {
  height: 88rpx;
  line-height: 88rpx;
  color: #999;
  padding-left:10rpx; 
  overflow: hidden;
  font-size: 24rpx;
  background: #fff;
  border-bottom: 1px solid #f2f2f2;
}

.container-list:nth-child(1) .container-list-parameter .car-item-list .carname{
  border-right: 1rpx solid #f2f2f2;
}

.right-text{
  text-align: right;
  padding-right: 10rpx;
  box-sizing: border-box;
}
.carname {
  display:block;
  /* text-align: right */
  /* width: 219rpx; */
  /* padding: 0 30rpx; */
}

.carval {
  /* width: 219rpx; */
  color: #242726;
  display:block;
  /* padding: 0 10rpx; */
}

.parameter-list {
  width: 219rpx;
  height: 244rpx;
  overflow: hidden;
  border-right: 1rpx solid #ccc;
  position: relative;
   border-bottom: 1rpx solid #ccc;
  
}
.parameter-list-add{
  width: 80rpx;
  height: 80rpx;
margin:50rpx 68rpx 10rpx 68rpx;
}

.parameter-list-name {
  font-size: 24rpx;
  color: #666;
  line-height: 36rpx;
  width: 180rpx;
  height: 144rpx;
  margin: 20rpx;
  overflow: hidden;
}

.parameter-list-delete {
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  width: 40rpx;
}

.tabclss-btn {
  position:fixed;
  bottom:30rpx;
  right:30rpx;
  z-index: 111;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background: #33adff;
  box-shadow:0 4px 6px 0 rgba(50,173,255,0.30);
}
.tabclass{
  background:#ffffff;
border-radius:20rpx;
width:580rpx;
  position:fixed;
  bottom:80rpx;
  right:80rpx;
  z-index: 100;
/* height:744rpx; */
padding: 10rpx 0;
}
.tabclass text{
  width: 290rpx;
  height:88rpx;
  display: inline-block;
  float: left;
  font-size: 34rpx;
  line-height: 88rpx;
  text-align: center;

}
.tabclose{
  width: 64rpx;
}

.nicemenu-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.hideClass {
  display: none;
}

.showClass {
  display: block;
  color: #000;
}


.more-cars{
font-size:24rpx;
color:#33adff;
text-align:center;
padding: 0 28rpx;
}

.diff {
  color:#ff0000;
}